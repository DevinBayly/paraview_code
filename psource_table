table = vtk.vtkTable()

table.SetNumberOfRows(5)
cols = []
for i in range(20):
    arr = vtk.vtkStringArray()
    arr.SetNumberOfValues(5)

    for i in range(5):
        arr.SetValue(i,"random info")
    
    arr.SetName("test"+str(i))
    output.AddColumn(arr)
    cols.append(arr)

for j,arr in enumerate(cols):
    for i in range(5):
        print(j)
        table.SetValue(i,j,arr.GetValue(i))

